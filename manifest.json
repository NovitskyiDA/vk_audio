{
  "name": "Remove VK audio",
  "description": "Adds remove current audio in VK",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Remove",
    "default_icon": "icon16.png"
  },
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
    "Play / Pause": {
      "suggested_key": {
        "windows": "Alt+S",
        "mac": "Alt+S",
        "chromeos": "Alt+S",
        "linux": "Alt+S"
      },
      "description": "Toogle Play / Pause"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+X",
        "mac": "Alt+X",
        "chromeos": "Alt+X",
        "linux": "Alt+X"
      }
    }
  },
    "manifest_version": 2
  }
